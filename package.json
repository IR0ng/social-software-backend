{
  "name": "codehelp-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "async-entities": "dotenv cross-var -- typeorm-model-generator -h %DB_HOST% -d %DB% -p %DB_PORT% -u %DB_USER% -x %DB_PWD% -e mysql -o src/db/entities --noConfig -a --strictMode ?",
    "migration:create": "typeorm migration:create src/db/migrations/Template",
    "migration:up": "typeorm-ts-node-commonjs migration:run -d src/db/dataSource.ts",
    "migration:down": "typeorm-ts-node-commonjs migration:revert -d src/db/dataSource.ts",
    "build": "tsc && tsc-alias"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/lib-storage": "^3.658.1",
    "@types/node": "^22.0.2",
    "@types/pg": "^8.11.6",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cross-var": "^1.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validation": "^4.1.1",
    "iso-3166-1-alpha-2": "^1.0.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^7.6.0",
    "nodemon": "^3.1.3",
    "pg": "^8.12.0",
    "socket.io": "^4.7.5",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typeorm-model-generator": "^0.4.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/iso-3166-1-alpha-2": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  }
}
